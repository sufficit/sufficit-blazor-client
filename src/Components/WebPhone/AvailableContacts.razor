@if (Available?.Any() ?? false)
{
    <MudStack Row="true">
        @foreach (var endpoint in Available)
        {
            <MudIconButton OnClick="() => VoiceCall(endpoint)" Class="mud-elevation-5" Style="padding: 0">
                <ContactAvatar Size="Size.Medium" ReferenceId="GetUserId(endpoint)"></ContactAvatar>
            </MudIconButton>
        }
    </MudStack>
}