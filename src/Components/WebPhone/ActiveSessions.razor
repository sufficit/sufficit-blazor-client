@if (AudioPlay)
{
    <audio id="sufficit-ringing" autoplay loop><source src="@AudioFile"></audio>
} else if (!AudioPlayExists)
{
    <audio id="sufficit-ringing" muted><source src="@AudioFile"></audio>
}
@if (Active.Any())
{    
    <MudStack Row="true">
        @foreach (var session in Active) {
            <MudIconButton OnClick="() => OnSessionSelected(session)" Class="mud-elevation-5" Style="padding: 0">
                <ContactAvatar Size="Size.Medium" ReferenceId="GetUserId(session.RemoteUser)"></ContactAvatar>
            </MudIconButton>
        }
    </MudStack>
}