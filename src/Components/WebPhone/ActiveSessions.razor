@if (AudioPlay)
{
    <audio autoplay loop><source src="@AudioFile"></audio>
}
@if (Active.Any())
{    
    <MudStack Row="true">
        @foreach (var session in Active) {
            <MudIconButton OnClick="() => Sessions.Monitor.Set(session.Id)" Class="mud-elevation-5" Style="padding: 0">
                <ContactAvatar Size="Size.Medium" ReferenceId="GetUserId(session.RemoteUser)"></ContactAvatar>
            </MudIconButton>
        }
    </MudStack>
}