<MudCardHeader Style="padding: 0;" data-section="@Destination.TypeName">
    <CardHeaderAvatar>
        <MudIcon Color="Color.Secondary" Icon="@Icon"></MudIcon>
    </CardHeaderAvatar>
    <CardHeaderContent>
        <MudText Typo="Typo.body2">@Destination.Title</MudText>
        <MudText Typo="Typo.subtitle2">@Destination.Description</MudText>
        <MudText Typo="Typo.caption">@Destination.Asterisk</MudText>
    </CardHeaderContent>
</MudCardHeader>