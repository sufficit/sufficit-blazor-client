<MudCard Elevation="25" Class="px-6 py-2">
    <MudCardHeader>
        <CardHeaderAvatar>
            <MudFab StartIcon="@Icons.Material.Filled.Domain" DisableElevation="true" OnClick="ChooseClicked"></MudFab>
        </CardHeaderAvatar>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">Qual é o contexto ?</MudText>
            <MudText Typo="Typo.subtitle1">Você precisa escolher cliente para exibir as informações</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardActions>
        <MudButton FullWidth="true" Variant="Variant.Outlined" OnClick="ChooseClicked">Escolher</MudButton>
    </MudCardActions>
</MudCard>