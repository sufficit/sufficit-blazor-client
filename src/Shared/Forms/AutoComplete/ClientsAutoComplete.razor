@inherits GenericAutoComplete<Sufficit.IIdTitlePair>
<MudAutocomplete T="Sufficit.IIdTitlePair"
                 Label="Cliente"
                 Value="@Value"
                 Variant="Variant.Outlined"
                 Margin="Margin.Dense"
                 SearchFunc="Search"
                 ValueChanged="CurrentValueChanged" 
                 Adornment="Adornment.Start"
                 AdornmentIcon="@Icons.Material.Filled.Search"
                 IconSize="Size.Small"
                 CoerceText="false"
                 MinCharacters="4"
                 ResetValueOnEmptyText="false"
                 ShowProgressIndicator="true"
                 Converter="@IIdTitlePairContactConverter.Instance"
                 HelperText="Mínimo de 4 caracteres">
    <NoItemsTemplate>
        <MudText Align="Align.Center" Class="px-4 py-1">
            Nenhum item encontrado
        </MudText>
    </NoItemsTemplate>
    <ItemTemplate>
        <IIdTitlePairItemTemplate Item="@context" />
    </ItemTemplate>
</MudAutocomplete>