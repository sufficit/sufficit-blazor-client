@if(Visible) {
    <MudPaper Elevation="0" Style="background-color: transparent;" Class="px-md-16 py-16 d-flex align-center justify-center mud-width-full">
        <MudCard Elevation="25" Class="px-6 py-2">
            <MudCardHeader>
                <CardHeaderAvatar>
                    <MudFab Icon="@Icons.Material.Filled.TripOrigin" DisableElevation="true" Href="@Pages.Telephony.DirectInwardDialing.Free.RouteParameter"></MudFab>
                </CardHeaderAvatar>
                <CardHeaderContent>
                    <MudText Typo="Typo.h6">Rotas disponíveis</MudText>
                    <MudText Typo="Typo.subtitle1">Visualizar as rotas livres para venda ou isoladas em espera</MudText>
                </CardHeaderContent>
            </MudCardHeader>
            <MudCardActions>
                <MudButton FullWidth="true" Variant="Variant.Outlined" Href="@Pages.Telephony.DirectInwardDialing.Free.RouteParameter">Navegar</MudButton>
            </MudCardActions>
        </MudCard>
    </MudPaper>
}